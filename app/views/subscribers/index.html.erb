<p id="notice"><%= notice %></p>

<h1>Subscribers</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Email</th>
      <th>News</th>
      <th>Events</th>
      <th>Snowboarding</th>
      <th>Skiing</th>
      <th>Motox</th>
      <th>Skateboarding</th>
      <th>Bmx</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subscribers.each do |subscriber| %>
      <tr>
        <td><%= subscriber.firstname %></td>
        <td><%= subscriber.lastname %></td>
        <td><%= subscriber.email %></td>
        <td><%= subscriber.news %></td>
        <td><%= subscriber.events %></td>
        <td><%= subscriber.snowboarding %></td>
        <td><%= subscriber.skiing %></td>
        <td><%= subscriber.motoX %></td>
        <td><%= subscriber.skateboarding %></td>
        <td><%= subscriber.bmx %></td>
        <td><%= link_to 'Show', subscriber %></td>
        <td><%= link_to 'Edit', edit_subscriber_path(subscriber) %></td>
        <td><%= link_to 'Destroy', subscriber, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subscriber', new_subscriber_path %>
